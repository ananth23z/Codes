#include <iostream>
#include<vector>
using namespace std;

int main()
{
    vector<vector<int>>adjMtx;  // given adj matrix
    int v;               // v is number of vertex
    vector<int>adjlist[v];   // created adj list
    for(int i=0;i<v;i++){
        for(int j=0;j<v;j++){
            if(adjmtx[i][j]==1 && i!=j){
                adjlist[i].push_back(j);
                adjlist[j].push_back(i);
            }
        }
    }
}
